+# Используем базовый образ
+FROM piegsaj/oracle-jre:latest
+LABEL authors="TheHeret1c"
+
+# Устанавливаем переменную окружения для порта базы данных
+ENV DB_PORT=3306
+
+# Копируем собранный JAR файл внутрь образа
+COPY build.jar /app/build.jar
+
+# Определяем рабочую директорию
+WORKDIR /app
+
+# Определяем команду для запуска приложения
+CMD ["java", "-jar", "build.jar"]
+
+# Открываем порт для базы данных
+EXPOSE 3306
